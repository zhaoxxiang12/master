            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">//---------------------删除Excel文件，防止数据重复添加-------------------------
String delete_path=vars.get(&quot;path&quot;);//  path为用户定义的变量；path赋值给delete_path
File delete_file = new File(delete_path); //定义删除文件的路径
delete_file.delete();  //删除Excel文件

//---------------------从数据库读取到的值并进行导出Excel------------------------
FileWriter fstream = new FileWriter(&quot;F:\\file\\用户登录账号.csv&quot;,true);//保存Excel文件的路径
BufferedWriter out = new BufferedWriter (fstream);
//out.write(vars.get(&quot;number_1&quot;)+&quot;\n&quot;);
//String a=vars.get(&quot;phone_&quot;+1);
String number1 = vars.get(&quot;number_#&quot;); //   变量“number_#”为需要遍历的次数
int number2 = Integer.parseInt(number1);//  将字符串number1转换为整数number2
//System.out.println(number2);

//遍历变量“number”的下标；number变量为从数据库查询到的所有值；number_ID为number变量的下标
for (int number_ID=1;number_ID&lt;=number2;number_ID++){
	out.write(vars.get(&quot;number_&quot;+number_ID)+&quot;\n&quot;);
}

out.close();
fstream.close();</stringProp>
