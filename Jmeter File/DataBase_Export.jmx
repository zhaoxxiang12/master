            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">//JMeter的内置API：prev.getResponseData()获取请求的响应内容
byte[] responseData = prev.getResponseData();

//导出的excel存放位置
private String filePath = &quot;F:/file/上报账户.csv&quot;;  
BufferedOutputStream bos = null;
FileOutputStream fos = null;
File file = null;
try {
   File file = new File(filePath);
   fos = new FileOutputStream(file); //不添加参数true，以非追加的方式添加内容
   bos = new BufferedOutputStream(fos);
   bos.write(responseData);
} catch (Exception e) {
   e.printStackTrace();
} finally {
   if (bos != null) {
      try {
         bos.close();
      } catch (IOException e1) {
         e1.printStackTrace();
      }
   }
   if (fos != null) {
      try {
         fos.close();
      } catch (IOException e1) {
         e1.printStackTrace();
      }
   }
}</stringProp>
